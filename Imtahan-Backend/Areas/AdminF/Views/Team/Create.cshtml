@model Team
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/AdminF/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <div class="row">
        <form method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly"></div>
            <div class="form-group">
                <label asp-for="Name">Name</label>
                <input asp-for="Name" class="form-control"   aria-describedby="emailHelp" placeholder="Enter name"> 
            </div>

             <div class="form-group">
                <label asp-for="Desc">Desc</label>
                <input asp-for="Desc" class="form-control"   aria-describedby="emailHelp" placeholder="Enter email"> 
            </div>

            <div class="form-group">
                <label asp-for="Position">Position</label>
                <input asp-for="Position" class="form-control"   aria-describedby="emailHelp" placeholder="Enter email"> 
            </div>

            
            <div class="form-group">
                <label asp-for="Photo" accept="image/*">Photo</label>
                <input asp-for="Photo" class="form-control"   aria-describedby="emailHelp" placeholder="Photo"> 
            </div>

            <div class="form-group">
                <label asp-for="Twitter">Twitter</label>
                <input asp-for="Twitter" class="form-control"   aria-describedby="emailHelp" placeholder="Enter Twitter"> 
            </div>

             <div class="form-group">
                <label asp-for="Instargarm">Instargarm</label>
                <input asp-for="Instargarm" class="form-control"   aria-describedby="emailHelp" placeholder="Enter Instargarm"> 
            </div>

            <div class="form-group">
                <label asp-for="Facebook">Facebook</label>
                <input asp-for="Facebook" class="form-control"   aria-describedby="emailHelp" placeholder="Enter Facebook"> 
            </div>

            
            <div class="form-group">
                <label asp-for="Linkedin">Linkedin</label>
                <input asp-for="Linkedin" class="form-control"   aria-describedby="emailHelp" placeholder="Linkedin"> 
            </div>

             
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>
